}

@ARTICLE{Abeel2009Robust,
  author = {Thomas Abeel and Thibault Helleputte and Yves Van de Peer and Pierre
	Dupont and Yvan Saeys},
  title = {Robust biomarker identification for cancer diagnosis with ensemble
